FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      checkPatchStatsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

patchName1
{
  type  patch; //faceSet
  centre (0 0 0);
  normal (0 0 1); //Use the standard surface normal, not the patch normal
}

patchName2
{
  type  patch; //faceSet
  centre (0 1 0);
  normal (0 0 -1);
}

faceSetName1
{
  type  faceSet; //patch
  centre (0 0 0);
  normal (0 1 0);
}


